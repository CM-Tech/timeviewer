{
  "compilerOptions": {
    "noEmit": true,
    "target": "ESNext",
    "module": "Preserve",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "types": ["vite/client"]
  }
}
